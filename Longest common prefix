class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        prefix = ""
        for i in range(len(strs[0])):
            current_char = strs[0][i]
            for s in strs:
                if i >= len(s) or s[i] != current_char:
                    return prefix
            prefix += current_char

        return prefix
