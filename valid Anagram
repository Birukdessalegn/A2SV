class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        sort_s = sorted(s)
        sort_t = sorted(t)
        n = len(s)
        if len(s) != len(t):
            return False
        for i in range(n):
            if sort_s[i] == sort_t[i]:
                continue
            else:
                return False
        return True
            
