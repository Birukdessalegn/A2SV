class Solution(object):
    def findRestaurant(self, list1, list2):
        result=[]
        minimum=float('inf')
        for i in range(len(list1)):
            for j in range(len(list2)):
                if list1[i] == list2[j]:
                    if (i+j < minimum):
                        result = [list1[i]]
                        minimum=i+j
                    elif (i+j == minimum):
                        result.append(list1[i])              
        return result
