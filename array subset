class Solution:
    # Function to check if b is a subset of a.
    def isSubset(self, a, b):
        freq = {}

        # Count frequency of elements in a
        for num in a:
            freq[num] = freq.get(num, 0) + 1

        # Check elements of b
        for num in b:
            if num not in freq or freq[num] == 0:
                return False
            freq[num] -= 1

        return True
