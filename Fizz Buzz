class Solution:
    def fizzBuzz(self, n: int) -> List[str]:
        result = []          # empty list to store answers

        for i in range(1, n + 1):   # loop from 1 to n
            if i % 3 == 0 and i % 5 == 0:
                result.append("FizzBuzz")
            elif i % 3 == 0:
                result.append("Fizz")
            elif i % 5 == 0:
                result.append("Buzz")
            else:
                result.append(str(i))  # convert number to string

        return result
